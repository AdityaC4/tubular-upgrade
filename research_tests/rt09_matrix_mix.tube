// Matrix-style nested loop with helper calls to stress pass ordering.
function MixCell(int row, int col) : int {
  return row * col + (row % 5) - (col % 3);
}

function main() : int {
  int row = 0;
  int total = 0;
  while (row < 80) {
    int col = 0;
    while (col < 80) {
      total = total + MixCell(row, col);
      col = col + 1;
    }
    row = row + 1;
  }
  return total; // 9,992,080
}
