// Loop with a heavier helper to expose interplay between inlining and unrolling.
function Mix(int value) : int {
  int term = value * value;
  term = term - value / 2;
  term = term + (value % 5) * (value % 7);
  return term;
}

function main() : int {
  int i = 0;
  int sum = 0;
  while (i < 8000) {
    sum = sum + Mix(i % 97);
    i = i + 1;
  }
  return sum; // 24,450,418
}
