// Register Pressure Test - Many variables with simple operations
// Tests if unrolling helps with register allocation and reduces memory traffic

function RegisterPressure() : int {
    int r1 = 1; int r2 = 2; int r3 = 3; int r4 = 4; int r5 = 5;
    int r6 = 6; int r7 = 7; int r8 = 8; int r9 = 9; int r10 = 10;
    int i = 0;
    while (i < 1500000) {  // Increased from 150K to 1.5M iterations (10x more!)
        r1 = r1 + i; r2 = r2 + i; r3 = r3 + i; r4 = r4 + i; r5 = r5 + i;
        r6 = r6 + i; r7 = r7 + i; r8 = r8 + i; r9 = r9 + i; r10 = r10 + i;
        i = i + 1;
    }
    return r1 + r2 + r3 + r4 + r5 + r6 + r7 + r8 + r9 + r10;
}
